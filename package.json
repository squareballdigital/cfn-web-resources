{
  "name": "@squareball/cfn-web-resources",
  "version": "0.1.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/squareballdigital/cfnassets.git"
  },
  "homepage": "https://github.com/squareballdigital/cfnassets#readme",
  "author": {
    "email": "gordon@squareball.co",
    "name": "Gordon Leigh"
  },
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "dist/*.zip",
    "lib/",
    "cjs/",
    "src/"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile && npm run compile:cjs && npm run lint && npm run bundle && npm run docs",
    "bundle": "cfnassets build --config assets.config.json",
    "clean": "rm -rf lib/ cjs/ *.tsbuildinfo",
    "compile": "npm run index && tsc",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "dev:docs": "concurrently --kill-others npm:watch:docs npm:serve:docs",
    "docs": "typedoc",
    "index": "node makeIndex.js src/",
    "lint": "eslint src/ --ext=ts",
    "serve:docs": "http-server docs/ -o",
    "watch:compile": "tsc -w",
    "watch:docs": "onchange src/**/*.ts -i -d 1000 -- npm run docs"
  },
  "devDependencies": {
    "@aws-sdk/client-acm": "^3.94.0",
    "@aws-sdk/client-dynamodb": "^3.94.0",
    "@aws-sdk/client-route-53": "^3.94.0",
    "@aws-sdk/client-s3": "^3.94.0",
    "@aws-sdk/lib-storage": "^3.94.0",
    "@fmtk/decoders": "^0.5.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@squareball/cfn-custom-resource": "^0.1.1",
    "@squareball/cfntypes": "^0.40.4",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.escaperegexp": "^4.1.7",
    "@types/micromatch": "^4.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.34",
    "@types/yauzl": "^2.10.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "builtin-modules": "^3.3.0",
    "concurrently": "^5.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "^0.12.3",
    "lodash.escaperegexp": "^4.1.2",
    "micromatch": "^4.0.5",
    "mime-types": "^2.1.35",
    "onchange": "^7.0.2",
    "prettier": "^2.1.2",
    "rollup": "^2.74.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.2",
    "yauzl": "^2.10.0"
  },
  "dependencies": {
    "@squareball/cfnassets": "^0.5.2",
    "@squareball/cfntemplate": "^0.5.0",
    "lodash.clonedeep": "^4.5.0"
  },
  "bundles": {
    "auto-cert": {
      "path": "dist/auto-cert.zip"
    },
    "empty-bucket": {
      "path": "dist/empty-bucket.zip"
    },
    "get-stream-arn": {
      "path": "dist/get-stream-arn.zip"
    },
    "put-object": {
      "path": "dist/put-object.zip"
    },
    "unpack-asset": {
      "path": "dist/unpack-asset.zip"
    }
  }
}
